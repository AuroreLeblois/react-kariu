{
  "name": "react-kariu",
  "private": false,
  "version": "0.4.5",
  "description": "react library for devs",
  "author": "Aurore Leblois",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "ui",
    "components",
    "effects"
  ],
  "publishConfig": {
    "@auroreleblois:react-kariu": "https://npm.pkg.github.com"
  },
  "copyright": "Copyright Â© year ${author}",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-css": "copyfiles -u 1 \"src/**/*.css\" dist/",
    "build": "npm run clean && tsup && npm run copy-css",
    "start": "start http://localhost:9999 && storybook dev --quiet --ci --port 9999",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles": "./dist/index.css"
  },
  "dependencies": {
    "@storybook/addon-onboarding": "^8.6.12",
    "moment": "^2.30.1",
    "styled-components": "^6.1.18",
    "swiper": "^11.2.6"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@chromatic-com/storybook": "^3.2.6",
    "@mdx-js/react": "^3.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-docs": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/theming": "^8.6.12",
    "@swc/core": "^1.11.22",
    "@types/react": "*18.2.66",
    "@types/react-dom": "*18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "chromatic": "^11.24.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.12.0",
    "rimraf": "^5.0.7",
    "storybook": "^8.6.12",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2"
  },
  "license": "ISC",
  "sideEffects": false,
  "module": "dist/index.mjs"
}
